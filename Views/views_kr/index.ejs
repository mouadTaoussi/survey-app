											<path id="Vector 6" d="M160.028 132.343L156.434 133.339L65.0264 100.969L68.1077 99.9725L160.028 132.343Z" fill="#1633FF"/>
											<g id="Group 5">
												<path id="Vector 7" d="M142.056 92.9971L115.353 85.0293L124.083 81.5425L146.678 89.0124L142.056 92.9971Z" fill="#1633FF"/>
