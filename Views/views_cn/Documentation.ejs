						<p class="text-left">it works</p>
					</div>
				</details>
				<!--  -->
				<details>
					<summary>
					  	1.0
					</summary>
					<div class="card my-4 p-4">
						<p class="text-left">it works</p>
					</div>
				</details>
			</section>


			<!-- Space -->
			<br>
			<br>
			<!-- Space -->

	</div>
